{
  "name": "@autoagent/mcp-server",
  "version": "1.0.0",
  "description": "MCP server for AutoAgent vehicle search",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "gen:key": "node -e \"console.log(require('crypto').randomBytes(32).toString('base64'))\""
  },
  "dependencies": {
    "@autoagent/shared": "workspace:*",
    "@modelcontextprotocol/sdk": "^0.4.0",
    "better-sqlite3": "^9.2.2",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "libsodium-wrappers": "^0.7.11",
    "nanoid": "^5.0.4",
    "pino": "^8.17.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^4.17.21",
    "@types/libsodium-wrappers": "^0.7.11",
    "@types/node": "^20.10.0",
    "@vitest/ui": "^1.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
